# Resend Email API Key
# Get this from https://resend.com/api-tokens
RESEND_API_KEY=your_resend_api_key_here

# Stripe Publishable Key (safe to be public)
NEXT_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Stripe Secret Key (KEEP PRIVATE - never commit this!)
STRIPE_SECRET_KEY=sk_test_your_secret_key_here

# Application URL (used for redirects and email links)
# Set to your production domain when deploying
NEXT_PUBLIC_URL=http://localhost:3000
